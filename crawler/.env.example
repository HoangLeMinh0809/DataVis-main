# DataVis Crawler Environment Variables
# Copy file này thành .env và điền các giá trị thực

# Debug mode (true/false)
DEBUG=false

# API Keys (nếu cần)
# API_KEY_1=your_api_key_here
# API_KEY_2=your_api_key_here

# Database (nếu cần lưu vào DB thay vì file)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=datavis
# DB_USER=user
# DB_PASS=password

# Email notifications
# SMTP_HOST=smtp.gmail.com
# SMTP_PORT=587
# SMTP_USER=your_email@gmail.com
# SMTP_PASS=your_app_password
# NOTIFY_EMAILS=admin@example.com,team@example.com

# Webhook notifications (Slack, Discord, etc.)
# WEBHOOK_URL=https://hooks.slack.com/services/xxx/yyy/zzz

# Timezone
TZ=Asia/Ho_Chi_Minh
